          <editable>true</editable>
        </parameter>
        <parameter>
          <name>warFile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>user</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>password</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>commitMessage</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>version</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>stage</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
