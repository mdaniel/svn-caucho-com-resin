          <editable>true</editable>
        </parameter>
        <parameter>
          <name>archive</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>writeHead</name>
          <type>java.lang.Boolean</type>
          <defaultValue>true</defaultValue>
          <required>false</required>
          <editable>true</editable>
        </parameter>
      </parameters>
      <configuration>
        <contextRoot implementation="java.lang.String">/${project.build.finalName}</contextRoot>
        <warFile implementation="java.lang.String">${project.build.directory}/${project.build.finalName}.war</warFile>
      </configuration>
    </mojo>

